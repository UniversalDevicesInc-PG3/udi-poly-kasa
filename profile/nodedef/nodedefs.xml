<nodeDefs>
    <!-- NODE DEF from 5.0 document http://wiki.universal-devices.com/index.php?title=V50_MAIN -->
    <!-- controller, this must mach the id in your Controller class-->
    <nodeDef id="TPLinkKasaController" nls="ctl">
        <editors />
        <sts>
			<st id="ST" editor="bool" />
		</sts>
        <cmds>
            <sends />
            <accepts>
              <cmd id="QUERY" />
              <cmd id="DISCOVER" />
              <cmd id="UPDATE_PROFILE" />
            </accepts>
        </cmds>
    </nodeDef>

    <nodeDef id="SmartStrip" nls="ss">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
            </accepts>
        </cmds>
    </nodeDef>

    <nodeDef id="SmartStripPlug" nls="ssp">
        <editors />
        <sts>
            <st id="ST" editor="bool" />
        </sts>
        <cmds>
            <sends />
            <accepts>
                <cmd id="DON" />
                <cmd id="DOF" />
            </accepts>
        </cmds>
    </nodeDef>

</nodeDefs>
